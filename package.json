{
  "name": "eagle-frontend",
  "version": "1.0.0",
  "description": "保全网前端程序，采用react redux",
  "scripts": {
    "test": "jest",
    "build": "npm run clean && cross-env NODE_ENV=production npm run build-prod",
    "build-all": "npm run gulp && npm run build",
    "build-dev": "node_modules/.bin/webpack --colors --progress --config webpack.config.js",
    "build-prod": "node_modules/.bin/webpack --colors --progress --config webpack.config.server.js",
    "server": "node ./dist/server/server.js",
    "dev": "babel-node server.dev.js",
    "gulp": "cross-env NODE_ENV=production gulp prepare && gulp copy-js && cross-env NODE_ENV=production gulp webpack && cross-env NODE_ENV=production gulp build",
    "clean": "rimraf ./dist/server/*",
    "start": "cross-env NODE_ENV=development babel-node ./server.js",
    "server:static": "node ./server-static.js"
  },
